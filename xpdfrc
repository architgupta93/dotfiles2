unbind	f				any
unbind	n				scrLockOff
unbind	n				scrLockOn
bind	n				any					findNext
bind	/				any					find
bind	n				any					findNext
bind	ctrl-n	                        any					findNext
bind	h				any					scrollLeft(16)
bind	l				any					scrollRight(16)
bind	k				any					scrollUp(16)
bind	j				any					scrollDown(16)
bind	ctrl-o                  	any					toggleOutline
bind	=				any					zoomIn
bind	+				any					zoomIn
bind	-				any					zoomOut
bind	W				any					zoomFitWidth
bind	H				any					zoomFitPage
bind    c                               any                                     toggleContinuousMode
bind    f                               any                                     toggleFullScreenMode
