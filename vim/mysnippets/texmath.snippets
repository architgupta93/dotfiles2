snippet eqr "equation ref" w
\eqref{eqn:${1}}
endsnippet

snippet vb "vector bold" w
\vb{${1:${VISUAL}}}
endsnippet

snippet bs "bold symbol" w
\boldsymbol{${1:${VISUAL}}}
endsnippet

snippet -> "right arrow" w
\rightarrow
endsnippet

snippet dv "derivative" w
\dv{${1:${VISUAL}}}{${2}}
endsnippet

snippet __ "subscript" iA
_{${1}}
endsnippet

snippet ^^ "superscript" iA
^{${1}}
endsnippet
